<keymap version="1" name="Windows copy" parent="$default">
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="CollapseAllRegions">
    <keyboard-shortcut first-keystroke="shift ctrl subtract" />
    <keyboard-shortcut first-keystroke="shift ctrl minus" />
    <mouse-shortcut keystroke="button5" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
    <mouse-shortcut keystroke="button4" />
  </action>
</keymap>